{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "net.reasonset.kolmics",
  "version": "0.0.1",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": false
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "debug.log",
    "filesystem.getStats",
    "filesystem.readDirectory",
    "filesystem.getAbsolutePath",
    "filesystem.readBinaryFile",
    "server.mount",
    "window.isFullScreen",
    "window.exitFullScreen",
    "window.setFullScreen"
  ],
  "globalVariables": {},
  "modes": {
    "window": {
      "title": "kolmics",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icons/appIcon.png",
      "enableInspector": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false
    },
    "browser": {
      "enable": false
    },
    "cloud": {
      "enable": false
    },
    "chrome": {
      "enable": false
    }
  },
  "cli": {
    "binaryName": "kolmics",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "6.5.0",
    "clientVersion": "6.5.0"
  }
}